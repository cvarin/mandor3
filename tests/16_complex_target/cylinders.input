[units]
@ 1.0		Positive => works as wavelengh in microns [10^-4 cm].
		Negative => works as critical electrons' density [cm^-3].

[mesh]
@ 100		imax: mesh size along X axis
@ 100		jmax: mesh size along Y axis
@ 100		kmax: mesh size along Z axis
